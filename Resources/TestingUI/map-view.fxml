<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #E5E5E5;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TestingUI.MapController">
   <children>
      <ImageView fx:id="fullMap" fitHeight="720.0" fitWidth="1280.0" layoutX="-170.0" layoutY="-13.0" onMouseClicked="#mapSelected" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <image>
            <Image url="@Map/FullMap.png" />
         </image>
      </ImageView>
      <ImageView fx:id="westernEurope" fitHeight="720.0" fitWidth="1280.0" mouseTransparent="true" onMouseClicked="#countrySelected" pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <image>
            <Image url="@Map/WesternEurope.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
